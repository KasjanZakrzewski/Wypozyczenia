@model WypozyczeniaAPI.Models.Pojazd

@{
    ViewData["Title"] = "Edycja";
    Layout = "~/Views/Shared/_Layout_admin.cshtml";
}

<h1>Edytuj</h1>

<h4>Pojazd</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Rejestracja" class="control-label"></label>
                <input asp-for="Rejestracja" class="form-control" />
                
            </div>
            <div class="form-group">
                <label asp-for="Marka" class="control-label"></label>
                <input asp-for="Marka" class="form-control" />
                
            </div>
            <div class="form-group">
                <label asp-for="Model" class="control-label"></label>
                <input asp-for="Model" class="form-control" />
                
            </div>
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control" />

            </div>
            <div class="form-group">
                <label asp-for="NS" class="control-label"></label>
                <input asp-for="NS" class="form-control" />
                
            </div>
            <div class="form-group">
                <label asp-for="WE" class="control-label"></label>
                <input asp-for="WE" class="form-control" />
                
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Powrót do listy</a>
</div>
